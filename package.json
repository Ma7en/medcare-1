{
    "name": "medcare",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "server": "json-server --watch src/data/cities.json --port 8000 --delay 500",
